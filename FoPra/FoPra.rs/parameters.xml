<?xml version="1.0" encoding="UTF-8" ?>
<parameters>
					<parameter name="_count_tiers" displayName="# tiers" type="int" 
						defaultValue="3" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="_c" displayName="C (for L function)" type="int" 
						defaultValue="2" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="_strategy" displayName="strategy" type="java.lang.String" 
						defaultValue="random" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$StringStringConverter"
									
		/>
				<parameter name="_max_demand_frequency" displayName="max frequency" type="int" 
						defaultValue="1" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="_randomness_demand_configurations" displayName="Randomness demand configuration" type="double" 
						defaultValue="0.01" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="_demand_frequency" displayName="Frequency" type="int" 
						defaultValue="1" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="randomSeed" displayName="Default Random Seed" type="int" 
						defaultValue="__NULL__"
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="_count_agents_per_tier" displayName="# agents per tier" type="int" 
						defaultValue="3" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="_random_selection" displayName="Random Selection (if false, gamma will be ignored)" type="boolean" 
						defaultValue="false" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$BooleanConverter"
									
		/>
				<parameter name="random_memory_entries" displayName="Random memory entries" type="boolean" 
						defaultValue="false" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$BooleanConverter"
									values="true false "
						
		/>
				<parameter name="_dynamic_demands_period" displayName="Ticks dynamic demand period" type="int" 
						defaultValue="960" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="_memory_size" displayName="Memory Size" type="int" 
						defaultValue="0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="_gamma" displayName="gamma" type="double" 
						defaultValue="0.8" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
		</parameters>


