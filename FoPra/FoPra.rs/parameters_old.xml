<?xml version="1.0" encoding="UTF-8" ?>
<parameters>
					<parameter name="randomSelection" displayName="randomSelection: if false gamma is ignored and always the best entry is choosen" type="boolean" 
						defaultValue="false" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$BooleanConverter"
									values="true false "
						
		/>
				<parameter name="C" displayName="C" type="int" 
						defaultValue="2" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="debug" displayName="debug" type="boolean" 
						defaultValue="false" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$BooleanConverter"
									values="false true "
						
		/>
				<parameter name="m" displayName="m" type="int" 
						defaultValue="3" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="RandomDemandConfigurations" displayName="RandomDemandConfigurations (changing frequency randomly)" type="double" 
						defaultValue="0.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="n" displayName="n" type="int" 
						defaultValue="3" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="learningRate" displayName="learningRate" type="double" 
						defaultValue="0.8" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="randomSeed" displayName="Default Random Seed" type="int" 
						defaultValue="__NULL__"
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="memorySize" displayName="memorySize" type="int" 
						defaultValue="0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="randomMemory" displayName="initialize memory with random entries" type="boolean" 
						defaultValue="false" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$BooleanConverter"
									values="true false "
						
		/>
				<parameter name="maxDemandFrequency" displayName="maxDemandFrequency (for Random Configurations)" type="int" 
						defaultValue="1" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="demandFrequency" displayName="demandFrequency" type="int" 
						defaultValue="1" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="strategy" displayName="Strategy" type="java.lang.String" 
						defaultValue="random" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$StringStringConverter"
									values="adaptive random static activation "
						
		/>
				<parameter name="gamma" displayName="gamma" type="double" 
						defaultValue="1.0E18" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="dynamicDemandsPeriod" displayName="dynamicDemandsPeriod" type="int" 
						defaultValue="960" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
		</parameters>


